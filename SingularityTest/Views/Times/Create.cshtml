@model SingularityTest.Models.Time

@{
    ViewData["Title"] = "Create";    
}



<h4> @TempData["Time"] </h4>
<hr />
<div class="row">
    <div class="col-md-4">

        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>           
            <div class="form-group">
                <input type="submit" value="Current Time" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
<br>
